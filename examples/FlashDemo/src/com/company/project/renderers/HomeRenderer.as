package com.company.project.renderers{	import framework.view.RendererBase;	import framework.controller.Controller;	import flash.text.TextField;	public class HomeRenderer extends RendererBase	{			public var menu_mc:Menu;		public var title_txt:TextField;		public var description_txt:TextField;				public function HomeRenderer( controller:Controller, width:int, height:int )		{			super( controller, width, height );		}				override public function render( source:String ):void		{			title_txt.text = data.title;			description_txt.text = data.description;		}	}}